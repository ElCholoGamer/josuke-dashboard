	"private": true,
	"scripts": {
		"build": "yarn build:server && yarn build:app",
		"build:server": "tsc ./src/server/server.ts --esModuleInterop --skipLibCheck --outDir ./src/server-prod/",
		"build:app": "webpack --config ./webpack/webpack.prod.ts",
		"start:dev": "webpack serve --config ./webpack/webpack.dev.ts",
		"serve:dev": "ts-node-dev --transpile-only --rs --respawn ./src/server/server.ts",
		"start": "node ./src/server-prod/server.js",
		"analyze": "cross-env ANALYZE=true yarn build"
	},
